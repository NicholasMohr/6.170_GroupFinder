<div id="user">

  <p><span>Welcome, </span><span>{{currentUser}}</span> (<a href="#" id="logout-link">Logout</a>)</p>
  <a href="#" class="info-page">edit info</a>

  <h1>All Projects</h1>
  {{#each allProjects}}
    <div class="project">
      <p>{{name}}</p><button class="join-project">Join</button>
    </div>
  {{/each}}

  <h1>Your Projects</h1>
  {{#each userProjects}}
    <div class="project">
      <p>{{name}} <button>Visit</button> </p>
    </div>
  {{/each}}

</div>
