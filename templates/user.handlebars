<div id="user">
  <p>Welcome, {{currentUser.authentication.username}} (<a href="#" id="logout-link">logout</a>)</p>
  <h1>Your Projects</h1>

  {{#each projects}}
    <div class="project">
      <p>{{_id}}</p>
      <p>{{name}}</p>
    </div>
  {{/each}}

  <h1>View your info</h1>
  <h1>Your Info</h1>
  <div data-info-id="name">
  	Name: <p>{{info.name}}</p>   <a href="#" class="edit-info">Edit</a>
  </div>
  <div data-info-id="email">
    Email: <p>{{info.email}}</p>   <a href="#" class="edit-info">Edit</a>
  </div>
  <div data-info-id="phone">
    Phone: <p>{{info.phone}}</p>   <a href="#" class="edit-info">Edit</a>
  </div>
  <div data-info-id="location">
    Location:<p>{{info.location}}</p>   <a href="#" class="edit-info">Edit</a>
  </div>
  <div data-info-id="availability">
    Availability: <p>{{info.availability}}</p>   <a href="#" class="edit-info">Edit</a>
  </div>
  <div data-info-id="skills">
    Skills: <p>{{info.skills}}</p>  <a href="#" class="edit-info"></a>
  </div>
  <div data-info-id="timing">
    Timing: <p>{{info.timing}}</p>  <a href="#" class="edit-info">Edit</a>
  </div>
</div>
