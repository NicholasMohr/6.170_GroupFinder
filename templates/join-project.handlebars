<div id="join-project">
  <div class="container">

    <div class="row">
      <span style="float:right"><a href="#" id="home-link">Home</a><a href="#" id="logout-link">Logout</a></span>
      <h1 class="title">GroupFinder</h1>
    </div>

    <div class="row" id="new_project_container">
      <div class="col-sm-12 center">

        <div class="row">
          <h3 class="center">Join Project</h3>
        </div>

        <div class="row">
          <div class="join-project-container">
            <div class="join-proj-info" data-info-id="desired_grade" min="0" max="10">
              <b>Desired Grade:</b> <br>
              <input type="range" />
            </div>
            <div class="join-proj-info" data-info-id="dedication" min="0" max="10">
              <b>Dedication:</b> <br>
              <input type="range" />
            </div>
            <div class="join-proj-info" data-info-id="interaction" min="0" max="10">
              <b>Interaction:</b> <br>
              <input type="range" />
            </div>
          </div>
        </div>

        <div class="row center">
          <button class="join-project-submit">Join</button>
        </div>

      </div>
    </div>

  </div>
</div>